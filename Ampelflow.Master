	<script>
		
		function ProgressOn() {
			
            $('#ProgressModal').modal('show');
		}
		function ProgressOff() {
            $('#ProgressModal').modal('hide');
		}


		function sync_poData() {
            $.ajax({
                url: '../../ampelflow_master_srv.asmx/sync_poData',
                method: 'post',
               /* data: { action: 'Display_GetMaterial_Item' },*/
                dataType: 'json',
				success: function (data) {
					if (data == '1') {
						Swal.fire({
                            title: '<h3 class="txtSecondHeader" style="color:#a5dc86"><strong>อัพเดทข้อมูลเรียบร้อย</strong></h3>',
							icon: 'success',
                            confirmButtonColor: '#a5dc86',
							cancelButtonColor: '#f94144',
							confirmButtonText: '<div class="txtLabel">ตกลง</div>'
						})
					} else if (data == '0') {
                        Swal.fire({
                            title: '<h3 class="txtSecondHeader" style="color:#f27474"><strong>เกิดข้อผิดพลาด</strong></h3>',
                            icon: 'error',
                            confirmButtonColor: '#f27474',
                            cancelButtonColor: '#f94144',
                            confirmButtonText: '<div class="txtLabel">ตกลง</div>'
                        })
                    }
                }
            })
		}

		function sync_customer_ampelflow() {
            $.ajax({
                url: '../../ampelflow_master_srv.asmx/sync_customer_ampelflow',
                method: 'post',
                /* data: { action: 'Display_GetMaterial_Item' },*/
                dataType: 'json',
                success: function (data) {
                    if (data == '1') {
                        Swal.fire({
                            title: '<h3 class="txtSecondHeader" style="color:#a5dc86"><strong>อัพเดทข้อมูลเรียบร้อย</strong></h3>',
                            icon: 'success',
                            confirmButtonColor: '#a5dc86',
                            cancelButtonColor: '#f94144',
                            confirmButtonText: '<div class="txtLabel">ตกลง</div>'
                        })
                    } else if (data == '0') {
                        Swal.fire({
                            title: '<h3 class="txtSecondHeader" style="color:#f27474"><strong>เกิดข้อผิดพลาด</strong></h3>',
                            icon: 'error',
                            confirmButtonColor: '#f27474',
                            cancelButtonColor: '#f94144',
                            confirmButtonText: '<div class="txtLabel">ตกลง</div>'
                        })
                    }
                }
            })
        }
    </script>

